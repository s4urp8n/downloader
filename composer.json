{
  "name": "zver/downloader",
  "description": "package to help download files from http, https, ftp",
  "minimum-stability": "stable",
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests/"
    }
  },
  "require": {
    "zver/common": "^3",
    "phpunit/phpunit": "^8.5",
    "ext-curl": "*"
  },
  "require-dev": {
  }
}
